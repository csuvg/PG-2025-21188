{
  "name": "santa-ana-forms-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "commit": "npx cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:mocha": "cross-env NODE_ENV=test VITE_API_BASE_URL=http://localhost:5173 mocha -r mocha_tests/setup-env.cjs -r mocha_tests/register-aliases.cjs -r mocha_tests/swc-register.cjs -r mocha_tests/register-assets.cjs -r mocha_tests/init-jsdom.cjs -r mocha_tests/register-stubs.cjs \"mocha_tests/mocha/**/*.{ts,tsx}\" --timeout 25000 --exit",
    "test:cypress:ci": "npm run build && start-server-and-test 'vite preview --port 5173' http://localhost:5173 'cypress run'",
    "test:cypress:open": "npm run build && start-server-and-test 'vite preview --port 5173' http://localhost:5173 'cypress open'",
    "test:cypress:dev": "start-server-and-test 'vite --port 5173' http://localhost:5173 'cypress run'",
    "test:selenium": "node selenium_tests/selenium/run-local.cjs",
    "test:selenium:lambdatest": "node selenium_tests/selenium/run-lt.cjs",
    "test:all": "yarn test && yarn test:mocha && yarn test:cypress:ci && yarn test:selenium"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
